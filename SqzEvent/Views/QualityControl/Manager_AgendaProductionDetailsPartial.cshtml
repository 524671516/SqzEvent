@model IEnumerable<SqzEvent.Models.ProductionDetails>


@foreach (var item in Model)
{
    <li>
        <div class="item-inner item-content">
            <div class="item-title">@Html.Encode(item.Product.SimpleName)</div>
            <div class="item-after">@Html.Encode(item.ProductionQty) 盒</div>
        </div>
    </li>
}