<div class="navbar">
    <div class="navbar-inner">
        <!-- We have home navbar without left link-->
        <div class="center sliding">历史记录</div>
    </div>
</div>
<!-- Pages, because we need fixed-through navbar and toolbar, it has additional appropriate classes-->
<div class="pages navbar-through toolbar-through">
    <!-- Page, data-page contains page name-->
    <div data-page="History" class="page" id="History">
        <!-- Scrollable page content-->
        <div class="page-content">
            <div class="list-block top-list">
                <ul>
                    <li>
                        <a href="/QualityControl/Manager_QualityTest" class="item-link item-content">
                            <div class="item-media"><i class="fa fa-check-square icon-css" aria-hidden="true"></i></div>
                            <div class="item-inner">
                                <div class="item-title">质检记录</div>
                                <div class="item-after"></div>
                            </div>
                        </a>
                    </li>
                    <li>
                        <a href="/QualityControl/Manager_Breakdown" class="item-link item-content">
                            <div class="item-media"><i class="fa fa-wrench icon-css" aria-hidden="true"></i></div>
                            <div class="item-inner">
                                <div class="item-title">故障记录</div>
                                <div class="item-after"></div>
                            </div>
                        </a>
                    </li>
                            <li>
                                <a href="/QualityControl/Manager_QualityRegularTest" class="item-link item-content">
                                    <div class="item-media"><i class="fa fa-wrench icon-css" aria-hidden="true"></i></div>
                                    <div class="item-inner">
                                        <div class="item-title">定期检测</div>
                                        <div class="item-after" id="qrt_count">@Html.Encode(ViewBag.QRT_Cnt)</div>
                                    </div>
                                </a>
                            </li>
                </ul>
            </div>
        </div>
    </div>
</div>
