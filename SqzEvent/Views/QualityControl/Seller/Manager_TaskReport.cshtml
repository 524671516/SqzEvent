@model SqzEvent.Models.Off_Manager_Task

<div class="navbar">
    <div class="navbar-inner">
        <div class="left">
            <a href="#" class="back link">
                <i class="icon icon-back" aria-hidden="true"></i>
                <span>返回</span>
            </a>
        </div>
        <div class="center sliding">督导工作日报</div>
    </div>
</div>
<div class="pages">
    <div class="page" data-page="manager-task-report" id="manager-task-report">
        <div class="page-content">
            <div class="content-block-title">选择日期</div>
            <div class="list-block">
                <ul>
                    <li>
                       <a href="#" class="item-link smart-select" data-back-on-select="true" data-back-text="返回" data-searchbar="true" data-searchbar-placeholder="搜索日期" data-searchbar-cancel="取消">
                           @Html.DropDownList("taskreport-date", ViewBag.checkinlist as SelectList, new { })
                           <div class="item-content">
                               <div class="item-inner">
                                   <div class="item-title">日期</div>
                               </div>
                           </div>
                       </a>
                    </li>
                </ul>
            </div>
            <div id="task_details">

            </div>
        </div>
    </div>
</div>