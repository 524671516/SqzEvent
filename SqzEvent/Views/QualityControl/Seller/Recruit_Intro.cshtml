                'https://cdn2.shouquanzhai.cn/SellerIntro/Seller_function3_01.jpg',
                'https://cdn2.shouquanzhai.cn/SellerIntro/Seller_function3_02.jpg',
                'https://cdn2.shouquanzhai.cn/SellerIntro/Seller_function3_03.jpg',
            ],
            theme: 'dark',
            backLinkText: '关闭',
            ofText: '/'
        });
        //点击时打开图片浏览器
        $$('.pb-function_3').on('click', function () {
            myPhotoBrowserFunction_3.open();
        });
        var myPhotoBrowserFunction_4 = myApp.photoBrowser({
            photos: [
                'https://cdn2.shouquanzhai.cn/SellerIntro/Seller_function4_01.jpg',
            ],
            theme: 'dark',
            backLinkText: '关闭',
            ofText: '/'
        });
        //点击时打开图片浏览器
        $$('.pb-function_4').on('click', function () {
            myPhotoBrowserFunction_4.open();
        });
    </script>
</body>

</html>
