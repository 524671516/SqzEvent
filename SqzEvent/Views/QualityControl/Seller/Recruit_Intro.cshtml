        });
        //点击时打开图片浏览器
        $$('.pb-offduty').on('click', function () {
            myPhotoBrowserOffDuty.open();
        });
        //功能区
        var myPhotoBrowserFunction_1 = myApp.photoBrowser({
            photos: [
                'https://cdn2.shouquanzhai.cn/SellerIntro/Seller_function1_01.jpg',
            ],
            theme: 'dark',
            backLinkText: '关闭',
            ofText: '/'
        });
        //点击时打开图片浏览器
        $$('.pb-function_1').on('click', function () {
