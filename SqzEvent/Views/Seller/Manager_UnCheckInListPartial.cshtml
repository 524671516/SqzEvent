@model IEnumerable<SqzEvent.Models.Off_Checkin_Schedule>
@if (Model.Count() == 0)
{
    <div class="list-block-label">本日无记录</div>
}
else
{<ul class="list-content">
    @foreach (SqzEvent.Models.Off_Checkin_Schedule item in Model)
    {
        <li class="swipeout">
            <div class="swipeout-content">
                <a href="/Seller/Manager_ScheduleSeller/@item.Id" class="item-content item-link">
                    <div class="item-inner">
                        <div class="item-title">@Html.Encode(item.Off_Store.StoreName)</div>
                        <div class="item-after">查看促销员</div>
                    </div>
                </a>
            </div>
            <div class="swipeout-actions-right">
                <a href="/Seller/Manager_CreateCheckIn/@item.Id" class="bg-blue">代签到</a>
            </div>
        </li>
    }
</ul>

}



