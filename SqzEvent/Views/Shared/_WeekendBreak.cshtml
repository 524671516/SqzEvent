@{
    ViewBag.Message = "周末突破统计程序";
    ViewBag.Title = "周末突破统计程序";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no, minimal-ui" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/weui.min.css" rel="stylesheet" />
    <link href="~/Content/daterangepicker-bs3.css" rel="stylesheet" />
    <style>
        body, html {
            height: 100%;
            -webkit-tap-highlight-color: transparent;
        }

        body {
            font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
            font-size:14px;
            line-height:22px;
        }

        ul {
            list-style: none;
        }
        .page, body {
            background-color: #f8f8f8;
        }

        .link {
            color: #1aad19;
        }

        .container {
            overflow: hidden;
        }

        .container, .page {
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
        }

        .page {
            overflow-y: auto;
            -webkit-overflow-scrolling: touch;
            opacity: 0;
            z-index: 1;
        }

            .page.js_show {
                opacity: 1;
            }

        .page__hd {
            padding: 30px;
        }

        .page__bd_spacing {
            padding: 0 15px;
        }

        .page__title {
            text-align: left;
            font-size: 20px;
            font-weight: 400;
        }

        .page__desc {
            margin-top: 5px;
            color: #888;
            text-align: left;
            font-size: 14px;
        }
        .page__header{
            padding:10px;
            font-size:16px;
            line-height:30px;
            background-color:#08f;
            text-align:center;
            color:#f8f8f8;
        }
        .weui-grid{
            padding:5px 10px;
            background-color:#FFF;
        }
        .weui-grid__icon{
            color:#08f;
            width:100%;
            font-size:36px;
            line-height:48px;
            height:48px;
            text-align:center;
        }
        .hidden{
            display:none;
        }
        .sale-input, .try-input{
            max-width:30px;
            text-align:center;
        }
        a.input-group-addon{
            color:#999;
            padding:0px 8px;
        }
        
        .c_num{
            color:#999;
            margin-right:60px;
            text-align:center;
            line-height:28px;
            height:28px;
        }
        .item-title{
            max-width:180px;
            width:100%;
            line-height:28px;
        }
        .weui-flex-home{
            padding:0px 15px;
            border-top:1px solid #d9d9d9;
            border-bottom:1px solid #d9d9d9;
            background-color:#fff;
        }
        .weui-flex-home .weui-flex__item .placeholder div{
            width:50%; float:left;
            line-height:48px;
        }
        .weui-flex-home .weui-flex__item .placeholder div.placeholder__content{
            text-align:center;
            color:#999;
        }
        .weui-flex-table{
            border-top:1px solid #d9d9d9;
            background-color:#fff;
        }
        .weui-flex-table .weui-flex-time{
            width:18%;
            border-right:1px solid #d9d9d9;
        }
        .weui-flex-table .weui-flex-num{
            width:12%;
            border-right:1px solid #d9d9d9;
        }
        .weui-flex-header{
            text-align:center;
            font-weight:bolder;
            line-height:36px;
        }
        .weui-flex-content{
            text-align:center;
            padding:5px 0;
        }
        .weui-flex-details{
            font-size:12px;
            padding:5px;
            color:#999;
        }
        .daterangepicker{
            display:none;
        }
        #iosActionsheetDelete{
            color:#ff0000;
        }
    </style>
</head>
<body>
    <div class="weui-toptips weui-toptips_warn js_tooltips">错误提示</div>
    <div class="container" id="container">
        <div class="page home js_show">
            @RenderBody()
        </div>
    </div>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.form.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <script src="~/Scripts/daterangepicker.js"></script>
    <script src="//res.wx.qq.com/open/js/jweixin-1.0.0.js" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</body>
</html>